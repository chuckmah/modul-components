{
    "tag": "m-timepicker",
    "name": "m-timepicker-meta:name",
    "attributes": {
        "time": {
            "type": "moment.Moment | moment.Duration",
            "default": "moment()"
        },
        "isDuration": {
            "type": "boolean",
            "default": false
        },
        "step": {
            "type": "number",
            "default": 5
        },
        "disabled": {
            "type": "boolean",
            "default": false
        },
        "format": {
            "type": "string",
            "values": "see Moment.js formating options",
            "default": "LT"
        },
        "options": {
            "type": "any",
            "values": "see Popper.js options",
            "default": {
                "placement": "bottom-start"
            }
        }
    },
    "overview": "timepicker.overview"
}
