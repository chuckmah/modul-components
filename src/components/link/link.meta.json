{
    "tag": "m-link",
    "name": "m-link-meta:name",
    "attributes": {
        "mode": {
            "type": "string",
            "values": [
                "router-link",
                "button",
                "external-link",
                "link"
            ],
            "default": 0
        },
        "url" : {
            "type": "string",
            "default": "/"
        },
        "unvisited" : {
            "type": "boolean",
            "values": [
                "true",
                "false"
            ],
            "default": 1
        },
        "underline" : {
            "type": "boolean",
            "values": [
                "true",
                "false"
            ],
            "default": 0
        },
        "noStyle": {
            "type": "boolean",
            "values": [
                "true",
                "false"
            ],
            "default": 0
        },
        "hiddenText" : {
            "type": "string"
        }
    },
    "overview": "link.overview"
}
